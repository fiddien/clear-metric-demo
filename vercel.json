{
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/wsgi.py"
        }
    ],
    "functions": {
        "api/*.py": {
        "excludeFiles": "{.git, __pycache__, api/__pycache__}"
        }
    }
}